# Example environment variables for BeatsChain server scaffold
# Web3.Storage token (server-side only)
WEB3STORAGE_TOKEN=YOUR_WEB3STORAGE_TOKEN_HERE

# Livepeer API key (server)
LIVEPEER_API_KEY=YOUR_LIVEPEER_API_KEY_HERE

# Thirdweb / blockchain server keys
THIRDWEB_SERVER_KEY=YOUR_THIRDWEB_KEY

# Firebase service account (for token verification) - multi-line JSON or path to file
# FIREBASE_SERVICE_ACCOUNT_JSON={...}

PORT=4000
